{
    "version": 2,
    "builds": [
        {
            "src": "api/latency.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/latency",
            "methods": [
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type"
            },
            "continue": true
        },
        {
            "src": "/api/latency",
            "dest": "api/latency.py",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type"
            }
        }
    ]
}